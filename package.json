{
  "name": "projet",
  "version": "1.0.0",
  "description": "Projet NodeJs API REST",
  "main": "./src/start.js",
  "scripts": {
    "start": "nodemon --exec babel-node -- ./src/start.js --watch ./src",
    "build": "babel ./src --out-dir ./compile --copy-files",
    "eslint": "eslint ./src",
    "eslint-fix": "npm run eslint -- --fix",
    "document": "jsdoc ./src -r -d ./documentation",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test-watch": "npm run test -- --watch"
  },
  "author": "LOMER / LECCIA / APRUZZESE",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "mongodb": "^2.2.33",
    "mongoose": "^4.13.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-node-debug": "^2.0.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0-beta.3",
    "eslint-plugin-import": "^2.8.0",
    "jsdoc": "^3.5.5",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1"
  }
}
